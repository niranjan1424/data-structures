#include<stdio.h>    
#include<stdlib.h>
int main(){
	int a=0,b=1,temp,n,i;
	printf("Enter the number: ");
	scanf("%d",&n);
	printf("Fibannaci series : ");
	if(n==1){
		printf("0");
	}
	else{
		if(n==2){
			printf("1");
		}
	else{
		printf("0 1");
		for(i=3;i<=n;i++){
		temp = a+b;
		printf(" %d",temp);
		a = b;
		b = temp;
        }
	}
    }
	
}
